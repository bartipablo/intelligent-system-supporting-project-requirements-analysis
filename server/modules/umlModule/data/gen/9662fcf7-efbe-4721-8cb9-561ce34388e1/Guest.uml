@startuml

actor Guest as G
rectangle "SearchForDogWalkers" {
    usecase "Search for Dog Walkers" as SDW
}
rectangle "Sign Up" {
    usecase "Fill out sign up form" as FOSUF
    usecase "Create account" as CA
}

G --> SDW : Search
G --> FOSUF : Sign Up
FOSUF --> CA : Submit form
SDW --> G : Results

@enduml